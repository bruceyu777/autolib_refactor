# Delete VM instance after test 
######################################################

[PC_KVM]
    comment: Shutdown FVM_A and FVM_B
    virsh shutdown FVM_A
    sleep 3
    virsh shutdown FVM_B
    sleep 3

    comment: Delete and remove storage 
    sudo virsh undefine FVM_B --remove-all-storage
    expect -e "FVM_B has been undefined" -for deletevm -t 5
    sleep 3

    sudo virsh undefine FVM_A --remove-all-storage
    expect -e "FVM_A has been undefined" -for deletevm -t 5
    sleep 3
   
    report deletevm

