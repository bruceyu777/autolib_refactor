    expect -e "outbandwidth=110000\\(kbps\\)" -for 945211 -t 5

    get system status

    diag netlink interface list FVM_A:PORT2 
    expect -e "inbandwidth=110000\\(kbps\\)" -for 945211 -t 5
    expect -e "outbandwidth=110000\\(kbps\\)" -for 945211 -t 5

    report 945211
   
