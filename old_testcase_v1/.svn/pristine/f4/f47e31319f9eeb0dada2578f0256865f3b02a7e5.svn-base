# 961817ï¼šVerify exe factoryreset2 keepvmlicense  
# fortiguard settting default 
# 
#############################

[FVM_A]
    
    comments: 961817 Verify factoryreset2 keepvmlicense
    # get license file name
    setlicense -t 02G -for SN -to lic    
    
    execute restore vmlicense tftp FVM_A:LICENSE_DIR{$lic} FVM_A:LICENSE_SERVER

    sleep 90	
    get system status | grep 'Serial-Number' -A 3
    expect -e "FGVM02T" -for 961817 -t 5
    expect -e "License Status: Valid" -for 961817 -t 5
    expect -e "2 CPU/2 allowed" -for 961817 -t 5
 
    config system global
        set admintimeout 479
    end

    execute factoryreset2 keepvmlicense

    get system status | grep 'Serial-Number' -A 3
    expect -e "FGVM02T" -for 961817 -t 5
    expect -e "License Status: Valid" -for 961817 -t 5

    show full system global | grep admintimeout
    expect -e "admintimeout 5" -for 961817 -t 5

    report 961817
   
