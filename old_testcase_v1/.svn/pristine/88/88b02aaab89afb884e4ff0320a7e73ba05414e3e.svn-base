# 1016625ï¼šVerify g04-license can be valided
# fortiguard settting default 
# 
#############################

[PC_KVM]
    ### 1016625.txt ###
    comments: Verify G04 can be valided base on cpu cores with default FGD setting

    # get license file name
    setlicense -t 04G -for SN -to lic
 

[FVM_A]
    #remove license
    execute factoryreset2    
    
    get system status | grep 'Serial-Number' -A 5
    expect -e "License Status: Invalid" -for 1016625 -t 5
    
    execute restore vmlicense tftp FVM_A:LICENSE_DIR{$lic} FVM_A:LICENSE_SERVER
	
    get system status | grep 'Serial-Number' -A 5
    expect -e "FGVM04T" -for 1016625 -t 5
    expect -e "License Status: Valid" -for 1016625 -t 5
    expect -e "4 CPU/4 allowed" -for 1016625 -t 5

    # VM01: default 10 vdoms, max 10 vdoms
    diag debug vm-print-license
    expect -e "Model: 04" -for 1016625 -t 5
    expect -e "CPU: 4" -for 1016625 -t 5
    expect -e "permanent: 10" -for 1016625 -t 5


    report 1016625
   
