# 812442ï¼šVerify gul-license show no limit on cpu 
# fortiguard settting default 
# 
#############################

[PC_KVM]
    ### 812442.txt ###
    comments: Verify GUL has no limit on cpu

    # get license file name
    #cat PC_KVM:LICENSE_DIR/PC_KVM:LIECENSE_FILE | grep 04V
    #setvar -e "(?n)^04V,(FGVM4V.*?lic)," -to lic
    setlicense -t ULG -for SN -to lic
 

[FVM_A]
    #remove license
    execute factoryreset2    
    
    get system status | grep 'Serial-Number' -A 5
    expect -e "License Status: Invalid" -for 812442 -t 5
    
    execute restore vmlicense tftp FVM_A:LICENSE_DIR{$lic} FVM_A:LICENSE_SERVER
	
    get system status | grep 'Serial-Number' -A 5
    expect -e "FGVMULT" -for 812442 -t 5
    expect -e "License Status: Valid" -for 812442 -t 5
    expect -e "4 CPU, " -for 812442 -t 5

    diag debug vm-print-license
    expect -e "Model: UL" -for 812442 -t 5
    expect -e "CPU: 2147483647" -for 812442 -t 5
    expect -e "permanent: 10" -for 812442 -t 5

    report 812442
   
