#945659
#######################################
[FVM_A]
    comment 945659:Busy:Verify IPv6 that is configured to interface work

    config system interface
        edit "FVM_A:PORT2"
            config ipv6
                set ip6-mode static
                set ip6-address FVM_A:IPV6_PORT2/64
                set ip6-allowaccess ping https ssh telnet
            end
        next
    end
	
[PC_01] 

    #########PING6###############
    ping6 -c 5 FVM_A:IPV6_PORT2
    expect -e "64 bytes from" -t 10 -for 945659

    ########TELNET##############
    telnet -6 -a -l admin FVM_A:IPV6_PORT2
    expect -e "login" -t 50 -for 945659
    sendctrlc
    sleep 3

    ########SSH#################
    echo "get system status" | sshpass -6 -p FVM_A:PASSWORD -o StrictHostKeyChecking=no admin@FVM_A:IPV6_PORT2
    expect -e "Version" -t 50 -for 945659
    sendctrlc

[FVM_A]
	
    get system status	 	 
    report 945659
