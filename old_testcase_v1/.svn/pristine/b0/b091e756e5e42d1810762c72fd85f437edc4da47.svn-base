# 1015666ï¼šVerify s32-license can be valided
# fortiguard settting default (anycast=enable)
# 
#############################

[PC_KVM]
    ### 1015666.txt ###
    comments: Verify s32 can be valided base on cpu cores with default FGD setting

    # get license file name
    #cat PC_KVM:LICENSE_DIR/PC_KVM:LIECENSE_FILE | grep 32S
    #setvar -e "(?n)^32S,(FGVMSL.*?lic)," -to lic
    setlicense -t 32S -for SN -to lic 

[FVM_A]
    #remove license
    execute factoryreset2    
    
    sleep 60

    get system status | grep 'Serial-Number' -A 3
    expect -e "Serial-Number: FGVMEV" -for 1015666 -t 5
    expect -e "License Status: Invalid" -for 1015666 -t 5
    
    # ev license not support anycast
    #config system fortiguard
    #    unset fortiguard-anycast
    #end
    
    execute restore vmlicense tftp FVM_A:LICENSE_DIR{$lic} FVM_A:LICENSE_SERVER
    sleep 60
	
    get system status | grep 'Serial-Number' -A 3
    expect -e "FGVMSL" -for 1015666 -t 5
    expect -e "License Status: Valid" -for 1015666 -t 5
    expect -e "1 CPU/32 allowed" -for 1015666 -t 5

    #s-license need to add cpu(see #596988)
    exe cpu add 3
    expect -e "Active CPU number: 4" -for 1015666 -t 5
    expect -e "Total CPU number: 4" -for 1015666 -t 5

    diag debug vm-print-license
    expect -e "Model: SL" -for 1015666 -t 5
    expect -e "CPU: 32" -for 1015666 -t 5

    report 1015666
   
