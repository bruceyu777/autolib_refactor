###################### 811521 ############################

[FVM_A]
	comment 811521:Busy:Verify AV Update through FDS works in NAT mode when set system dns

# database must be enabled and proxy av profile must be assigned
# in order for update to take effect
config antivirus profile 
   edit "av_test"
        set feature-set proxy 
        config http
            set av-scan block
        end
    next
end

config firewall policy
    edit 811521
        set name "811521"
        set srcintf "FVM_A:PORT2"
        set dstintf "FVM_A:PORT1"
        set srcaddr "all"
        set dstaddr "all"
        set action accept
        set schedule always
        set service "ALL"
        set inspection-mode proxy
        set utm-status enable
        set av-profile "av_test"
        set ssl-ssh-profile "certificate-inspection"
        set nat enable
    next
end
 

