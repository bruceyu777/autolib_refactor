#    sysctl mv /data2/virext /data2/virext.tmp -f
#    sysctl mv /data2/virexdb /data2/virexdb.tmp -f
#<elif FVM_A:Image eq out>
    fnsysctl mv /data2/vir /data2/vir.tmp -f
    admin
    FVM_A:Password
    sysctl mv /data2/virext /data2/virext.tmp -f
    admin
    FVM_A:Password
    sysctl mv /data2/virexdb /data2/virexdb.tmp -f
    admin
    FVM_A:Password
#<fi>

    sleep 3
    diag autoupdate version | grep 'Virus Definitions' -A 6
    expect -e "Version: 0.00000" -for 811521 -t 10

    diag debug app update 2

    #exe update won't update if last update within 5 minutes
    #clear cache to make sure exe update-av work
    diag test app update 4 
    setvar -e "expiry-time=(\\d{1,2})" -to time
    <if time eq 20>
        diag test app update 4
        expect -e "expiry-time=0" -for 811521 -t 5
    <fi>

    exec update-av

    sleep 180

    diag autoupdate version | grep 'Virus Definitions' -A 6
    expect -e "Result: Updates Installed" -for 811521 -t 10

    diag autoupdate version | grep 'Extended set' -A 6
    expect -e "Result: Updates Installed" -for 811521 -t 10


    diag deb reset

    config firewall policy
        delete 811521
    end

    report 811521
