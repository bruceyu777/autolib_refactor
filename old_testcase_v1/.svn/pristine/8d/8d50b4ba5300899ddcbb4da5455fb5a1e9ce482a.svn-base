# 960181ï¼šVerify g32-license can be valided
# fortiguard settting default 
# 
#############################

[PC_KVM]
    ### 960181.txt ###
    comments: Verify G32 can be valided base on cpu cores with default FGD setting

    # get license file name
    #cat PC_KVM:LICENSE_DIR/PC_KVM:LIECENSE_FILE | grep 04V
    #setvar -e "(?n)^04V,(FGVM4V.*?lic)," -to lic
    setlicense -t 32G -for SN -to lic
 

[FVM_A]
    #remove license
    execute factoryreset2    
    
    get system status | grep 'Serial-Number' -A 5
    expect -e "License Status: Invalid" -for 960181 -t 5
    
    execute restore vmlicense tftp FVM_A:LICENSE_DIR{$lic} FVM_A:LICENSE_SERVER
	
    get system status | grep 'Serial-Number' -A 5
    expect -e "FGVM32T" -for 960181 -t 5
    expect -e "License Status: Valid" -for 960181 -t 5
    #expect -e "4 CPU/32 allowed" -for 960181 -t 5

    diag debug vm-print-license
    expect -e "Model: 32" -for 960181 -t 5
    expect -e "CPU: 32" -for 960181 -t 5

    #report 960181

    # 812970: apply vdom license
    setlicense -t 32G -for VDOM -to vdlic

    # import vdom license
    execute upd-vd-license {$vdlic}
    y
    sleep 10

    get system status | grep 'domain'
    expect -e "Max number of virtual domains: 15" -for 812970 -t 10

    diag debug vm-print-license
    #expect -e "permanent: 15" -for 812970 -t 5

    report 960181
    report 812970
 
   
