# 1016627ï¼šVerify g01-license can be valided
# fortiguard settting default 
# 
#############################

[PC_KVM]
    ### 1016627.txt ###
    comment: Verify G01 can be valided base on cpu cores with default FGD setting

    # get license file name
    setlicense -t 01G -for SN -to lic
 

[FVM_A]
    #remove license
    execute factoryreset2    
    
    get system status | grep 'Serial-Number' -A 5
    expect -e "License Status: Invalid" -for 1016627 -t 5
    
    execute restore vmlicense tftp FVM_A:LICENSE_DIR{$lic} FVM_A:LICENSE_SERVER
	
    get system status | grep 'Serial-Number' -A 5
    expect -e "FGVM01T" -for 1016627 -t 5
    expect -e "License Status: Valid" -for 1016627 -t 5
    expect -e "1 CPU/1 allowed" -for 1016627 -t 5

    # VM01: default 10 vdoms, max 10 vdoms
    diag debug vm-print-license
    expect -e "Model: 01" -for 1016627 -t 5
    expect -e "CPU: 1" -for 1016627 -t 5
    expect -e "permanent: 10" -for 1016627 -t 5


    report 1016627
   
